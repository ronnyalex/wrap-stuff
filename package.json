{
  "name": "wrap-stuff",
  "displayName": "wrap stuff",
  "description": "wrapping string to log the output, wrap name and value. Wrap with template literals",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.86.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ronnyalex/log-wrap.git"
  },
  "publisher": "1tsg00d",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "log.wrap",
        "title": "Log wrap name and value"
      },
      {
        "command": "log.wrap.name",
        "title": "log wrap name"
      },
      {
        "command": "template.literal.wrap",
        "title": "Wrap with template literals"
      },
      {
        "command": "code.block.wrap",
        "title": "Wrap with code block"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "patch": "npm version patch",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
